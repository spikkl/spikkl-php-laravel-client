{
  "name": "spikkl/spikkl-php-laravel-client",
  "description": "Spikkl API client wrapper for Laravel",
  "homepage": "https://github.com/spikkl/spikkl-php-laravel-client",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Spikkl",
      "email": "support@spikkl.com"
    }
  ],
  "keywords": [
    "spikkl",
    "validation",
    "postal code",
    "laravel",
    "api",
    "client"
  ],
  "require": {
    "ext-json": "*",
    "php": "^7.1.3 | ^7.2",
    "illuminate/support": "^5.8.0 | ^6.0",
    "spikkl/spikkl-php-client": "^1.0"
  },
  "require-dev": {
    "graham-campbell/testbench": "^4.0|^5.0",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.5 | ^8.0"
  },
  "autoload": {
    "psr-4": {
      "Spikkl\\Laravel\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable" : true,
  "extra": {
    "laravel": {
      "providers": [
        "Spikkl\\Laravel\\ServiceProvider"
      ],
      "aliases": {
        "Spikkl": "Spikkl\\Laravel\\Facades\\Spikkl"
      }
    }
  }
}
